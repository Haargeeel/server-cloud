{
  "name": "server-cloud",
  "version": "0.0.1",
  "description": "Node Server",
  "main": "index.js",
  "scripts": {
    "start": "bnr start",
    "dev": "concurrently --raw --kill-others \"npm run webpack-dev\" \"npm run start-dev\"",
    "webpack-dev": "bnr webpack-dev",
    "start-dev": "bnr start-dev",
    "build": "bnr build"
  },
  "betterScripts": {
    "webpack-dev": {
      "command": "node webpack/dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-dev": {
      "command": "nodemon -w lib/ index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "./node_modules/.bin/webpack --colors --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "haargeeel <ray.g@gmx.de>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.2",
    "express": "^4.14.0",
    "mongodb": "^2.2.11",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "better-npm-run": "^0.0.11",
    "clean-webpack-plugin": "^0.1.13",
    "concurrently": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "piping": "^1.0.0-rc.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "standard": "^8.4.0",
    "standard-loader": "^5.0.0",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-isomorphic-tools": "2.5.7"
  }
}
